                  <% elsif profession.star == 3 then %>
                    <label for="star1"><span class="text" style="display: block; float: center;">容易度</span><span class= "star-color" >★★★</span></label>
                  <% elsif profession.star == 2 then %>
                    <label for="star1"><span class="text" style="display: block; float: center;">容易度</span><span class= "star-color" >★★</span></label>
                  <% else %>
                    <label for="star1"><span class="text" style="display: block; float: center;">容易度</span><span class= "star-color" >★</span></label>
                  <% end %>
                </div>
                <div style="display: inline-block; text-align: center; width: 150px;">
                  <% if profession.fun == 5 then %>
                    <label for="star1"><span class="text" style="display: block; float: center;">実用度</span><span class= "star-color" >★★★★★</span></label>
                  <% elsif profession.fun == 4 then %>
                    <label for="star1"><span class="text" style="display: block; float: center;">実用度</span><span class= "star-color" >★★★★</span></label>
                  <% elsif profession.fun == 3 then %>
                    <label for="star1"><span class="text" style="display: block; float: center;">実用度</span><span class= "star-color">★★★</span></label>
                  <% elsif profession.fun == 2 then %>
                    <label for="star1"><span class="text" style="display: block; float: center;">実用度</span><span class= "star-color">★★</span></label>
                  <% else %>
                    <label for="star1"><span class="text" style="display: block; float: center;">実用度</span><span class= "star-color">★</span></label>
                  <% end %>
                </div>
             </div>
             
             <div style="width: 300px; height: 50px; padding-left: 143px; margin-top: 35px;">
              <%= link_to "次へ", "/professions/#{profession.id}/top", class: "bogo" %>
             </div>
             
            </div>
           </div>
          
          </div>
        <% end %>
