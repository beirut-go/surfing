                        <h2 class="pdf-header1">個人情報</h2>
                      </div>
                      <div class="pdf-body">
                        <div class="pdf-body1">
                          <label class="name2">名前</label>
                          <div class="name_form">
                            <input placeholder="Name" type="string" name="lastname" value="<%= @guiders.lastname %>" autofocus="true" class="name_form1" required>
                          </div>
                        </div>

                        <div class="pdf-body1">
                          <label class="name2">苗字</label>
                          <div class="name_form">
                            
                            <input placeholder="Name" type="string" name="student_name" value="<%= @guiders.student_name %>" autofocus="true" class="name_form1" required>
                          </div>
                        </div>

                        <div class="pdf-body1" style="margin-bottom: 0px;">
                          <label class="name2">性別</label>
                          <% if Guider.find_by(gender: params[:gender]).blank? %>
                          <div class="select">
                           
                            <select name="gender", id="user_sex" value="<%= @guiders.gender %>" required>
                              <option value="性別">性別</option>
                              <option value="男">男性</option>
                              <option value="女">女性</option>
                              <option value="その他">その他</option>
                            </select>
                          </div>
                          <% else %>
                             <p class="birth"><input placeholder="性別" style="margin-left: 6px;" type="string" name="gender" value="<%= @guiders.gender %>" autofocus="true" class="name_form1" required></p>
                           
                          <% end %>
                        </div>
　　　　　　　　　　　
                        <div class="pdf-body1">
                          <label class="name2">職業</label>
