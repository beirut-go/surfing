                          <div class="select">
                            <select name="guider[occupation]" id="user_sex" value="<%= @guiders.occupation %>"  required>

                              <option value="職業">職業</option>
                              <option value="学生">学生</option>
                              
                            </select>
                          </div>
                          <% else %>
                            <div class="name_form">
                            <input placeholder="職業"  type="string" name="occupation" value="<%= @guiders.occupation %>" class="name_form1" autofocus="true" required>
                          </div>
                          <% end %>
                        </div>

                        <div class="pdf-body1">
                          <label class="name2">生年月日</label>
                          <div class="name_form">
                            <input placeholder="" type="string" name="birth" value="<%= @guiders.birth %>" autofocus="true" class="name_form1" required>
                          </div>
                        </div>

                        <div class="pdf-body1">
                          <label class="name2">メール</label>
                          <div class="name_form">
                             <input placeholder="Gmail" type="text" name="contact" value="<%= @guiders.contact %>" autofocus="true" class="name_form1" required>
                          </div>
                        </div>

                        <div class="pdf-body1">
                          <label class="name2">住所</label>
                          <div class="describe">
                             <input placeholder="" type="text" name="hometown" value="<%= @guiders.hometown %>" autofocus="true" class="describe1" required>
                          </div>
                        </div>


                      </div>
                    
                      <div class="pdf-header">
                        <h2 class="pdf-header1">本人情報</h2>
                      </div>
                      <div class="pdf-footer2">
                        <div class="pdf-body1">
                          <label class="name2">大学</label>
                          <div class="name_form">
                            <input placeholder="" type="string" name="university" value="<%= @guiders.university %>" autofocus="true" class="name_form1" required>
                          </div>
                        </div>

                        <div class="pdf-body1">
                          <label class="name2">学部</label>
                          <div class="name_form">
                            <input placeholder="" type="string" name="faculty" value="<%= @guiders.faculty %>" autofocus="true" class="name_form1" required>
                          </div>
                        </div>

                        <div class="pdf-body1">
                          <label class="name2">学年</label>
                          <div class="name_form">
                            <input placeholder="" type="string" name="year" value="<%= @guiders.year %>" class="name_form1" autofocus="true" required>
                          </div>
                        </div>

                      </div>


                      <div class="pdf-footer">
                        <h2 class="pdf-header1">本人確認書類</h2>
                      </div>
                      <div class="pdf-footer1">
                       <div class="pdf-f1">
                        
                          <div class="gogo1">
                            <div class="gogo2" >
                              <%= image_tag(@guiders.photo.url, class: "bookdo2") %>
                            </div>
                          </div>
                          <div class="image_form">
                            <div class="setumei">
                               <p class="image_setumei">以下の書類のうち、住所、氏名、生年月日、書類発行元が確認できるものをご用意ください。
                                </p>
                               
                               <p class="image_setumei2" style="margin-top: 30px;">・学生証</p>

                               
                            </div>
                            <div class="file1">
                                <%= file_field_tag "Image", class: "self", placeholder: "Upload a file from your computer", type: "text", :autofocus => true %>
                               
                            </div>
                          </div>
                        
                       </div>
                      </div>
                      <div class="pdf-footer">
                        <h2 class="pdf-header1">本人確認書類</h2>
                      </div>
                      <div class="pdf-footer1">
                       <div class="pdf-f1">
                        
                          <div class="gogo1">
                            <div class="gogo2" >
                             <%= image_tag(@guiders.driver.url, class: "bookdo2") %>
                            </div>
                          </div>
                          <div class="image_form">
                            <div class="setumei">
                                <p class="image_setumei">以下の書類のうち、住所、氏名、生年月日、書類発行元が確認できるものをご用意ください。
                                </p>
                               <p class="image_setumei1">・運転免許証</p>
                               <p class="image_setumei2" style="margin-top: 20px;">・健康保険証</p>
              
                            </div>
                            <div class="file1">
                                <%= file_field_tag "Image", class: "self", placeholder: "Upload a file from your computer", type: "text", :autofocus => true %>
                            </div>
                          </div>
                        
                       </div>
                      </div>
                      <div style="width: 766px; text-align: right; padding-right: 40px; padding-top: 10px;">
                         <button type="sumbit" value="Save" class="save">Save</button>
                      </div>

                  </div>

                  
                <% end %>
             </div>
          </div>
        </div>
     </div>
  </div>

</div>
