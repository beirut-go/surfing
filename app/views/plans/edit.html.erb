<div class="contents row">
    <%= form_tag("/plans/#{@plan.id}", method: :patch ) do %>
      <h3>
        編集する
      </h3>
      <input placeholder="Title" type="text" name="title" value="<%= @plan.title %>" autofocus="true">
      <input placeholder="ImageUrl" type="string" name="image" autofocus="true">
      <input placeholder="Place" type="string" name="place" autofocus="true">
      <input placeholder="MeetingPlace" type="string" name="meetingplace" autofocus="true">
      <input placeholder="How many hour" type="integer" name="times" value="<%= @plan.times %>" autofocus="true">
      <input placeholder="date" type="integer" name="datetimes" autofocus="true">
      <textarea cols="30" name="contents" placeholder="content" rows="10"><%= @plan.contents %></textarea>
      <input type="submit" value="SENT">
    <% end %>
</div>