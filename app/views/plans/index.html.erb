<div class="pc">
<div class="mainpage" style="height: 1650px;">
 
  <div class="contents center" style="width: 1100px; padding-left: 90px;">
   <% if @search_keyword.present? %> 
    <div class="topic">
     <span class="topic2"><%= @search_keyword %></span>
     <span class="topic1">の検索結果</span>
    </div>
   <% end %>
