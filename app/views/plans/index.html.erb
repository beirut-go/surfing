    <div style="position: relative; height: 233.91px; background-image: url(<%= plan.image.url %>);" >
    <%= image_tag(plan.image.url, class: "bookdo") %>
    <%= link_to '', "/plans/#{plan.id}", method: :get, class: "magic-master" %>
        <div class="more">
          
           <ul class="more_list">
            <li>
              <%= link_to '', "/plans/#{plan.id}", method: :get %>
            </li>
            
           </ul>
        </div>
       
      <span class="name">
        <a href="/guiders/<%= plan.guider_id %>">
          
        </a>
      </span>
      
    </div>
    

    <div class="mix-change">
     <span class="mix" style="margin-left: 0px;"><%= plan.title %></span>
     <span class="mix1">¥<%= plan.price %></span>
     
    </div>
     
   </div>


   <% end %>
   
   <%= paginate(@plans) %>
  
 </div>
